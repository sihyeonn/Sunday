extends ./layout/board.pug
block title
  div.jumbotron.my-2
    h2= title
      small - View Content
block content
  .boarder.boarder-primary.rounded.p-3= data.title
  .boarder.boarder-primary.rounded.p-3.my-3= data.wDate
  .boarder.boarder-primary.rounded.p-3.my-3= data.content
  .p-3.text-center
    button.btn.btn-primary#btUpdate Modify
    button.btn.btn-danger#btDelete Delete
    button.btn.btn-success#btList List
